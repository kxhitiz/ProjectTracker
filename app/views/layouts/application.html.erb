<!DOCTYPE html>
<html>
<head>
  <title>ProjectTracker</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag 'jquery', 'jquery-ui', 'rails', 'application' %>
  <%= csrf_meta_tag %>
</head>
<body>
<div id="head">
  <div id="head_cen">
    <div id="head_sup" class="head_pad">
      <p class="search">
        <label>SEARCH</label>
        <input name="search" type="text" class="txt" />
        <input name="search-btn" type="submit" class="btn" value="SEARCH" />
      </p>
      <h1 class="logo"><a href="#">PTracker</a></h1>
      <ul>
        <li><a class= "active" href="index.html">Home</a></li>
        <li><a href="">ABOUT</a></li>
        <li><a href="">SignIn</a></li>
      </ul>
    </div>
  </div>
</div>
<div id="content">
  <div id="content_cen">
    <div id="content_sup" class="head_pad">
      <div id="welcom_pan">

        <h2><span>Welcome</span>to ProjectTracker</h2>
      </div>
      <div>
        <%= yield %>
      </div>

    </div>
  </div>
</div>
<div id="foot">
  <div id="foot_cen">

    <p>&copy; Copyright Sprout Technology Services</p>
  </div>
</div>
</body>
</html>

